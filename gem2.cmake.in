FIND_LIBRARY(gem2_LIBRARY gem2
    PATHS @CMAKE_INSTALL_PREFIX@/lib
    NO_DEFAULT_PATH
)
FIND_LIBRARY(gem2_DEBUG_LIBRARY gem2d
    PATHS @CMAKE_INSTALL_PREFIX@/lib
    HINTS @CMAKE_INSTALL_PREFIX@/debug/lib
    NO_DEFAULT_PATH
)
SET(gem2_LIBRARIES ${gem2_LIBRARY})
FIND_PATH(gem2_INCLUDE_DIR @PROJECT_NAME@/gem2.h
    PATHS @CMAKE_INSTALL_PREFIX@/include
    NO_DEFAULT_PATH
)
SET(gem2_INCLUDE_DIRS ${gem2_INCLUDE_DIR})